import java.util.*;

public abstract class Animal {

	public Animal(String sound) {
		
		_subClasses.add(getClass());	
		_sound = sound;
	}
	
	String getSound() {
		return _sound;
	}
	
	void Speak() {
		System.out.println("");
	}
	
	static List<String> getAllowedAnimals(){
		
	}
	
	private String _sound;
	static List<Object> _subClasses= new LinkedList<Object>();

}
